function getBox(type,film)
{
clearTimeout(box_timer);
box_timer=setTimeout(function(){
	$('#movie_cash').show();
	$('#movie_cash').html('<div style="height: 100px"><br /><br /><br /><center><img src="/images/loaders/lightor.gif"></center></div>');
	$.get('/handler_get_parameter.php',{
		what:"box",
		where:type,
		id_film:84991
	},function(data){
	$('#movie_cash').html(data);
	});
	},750);
}
</script>
<div style="display: none" id="movie_cash"></div>


